########################################
# Open WebUI Configuration
########################################

# Secret key for securing the web UI (generate your own)
WEBUI_SECRET_KEY=""

# Allowed CORS origin (e.g. your frontend URL)
CORS_ALLOW_ORIGIN=""

# Custom User-Agent string (optional)
USER_AGENT=""

########################################
# LiteLLM Configuration
########################################

# LiteLLM API base URL
LITELLM_BASE_URL="http://host.docker.internal:4000/v1"

# Master key used for authentication
LITELLM_MASTER_KEY=""

# Salt key for hashing/symmetric operations
LITELLM_SALT_KEY=""

# Store model info in database (True/False)
STORE_MODEL_IN_DB=True

# Enable Prometheus metrics (True/False)
ENABLE_PROMETHEUS_METRICS=True

########################################
# PostgreSQL Configuration
########################################

# Connection string to Postgres
# Format: postgresql://USER:PASSWORD@HOST:PORT/DBNAME
DATABASE_URL="postgresql://<POSTGRES_USER>:<POSTGRES_PASSWORD>@db:5432/<POSTGRES_DB>"

# Database name
POSTGRES_DB=""

# Database username
POSTGRES_USER=""

# Database password
POSTGRES_PASSWORD=""
